import java.util.Scanner;

/**
 *
 * @author Tran Tien Dung
 */

public class Ex18 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        char c = sc.nextLine().charAt(0);
        if(c >= 'A' && c <= 'Z'){
            System.out.println(c += 32);
        }
        else if(c >= 'a' && c <= 'z'){
            System.out.println(c -= 32);
        }
        else{
            System.out.println(c);
        }
    }
}
