import java.util.Scanner;

/**
 *
 * @author Tran Tien Dung
 */

public class Ex12 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int x = sc.nextInt(), y = sc.nextInt();
        long f = 5L * x + 2L * y + 1L * x * y;
        System.out.println(f);
    }
}
