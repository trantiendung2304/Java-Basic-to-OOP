import java.util.Scanner;

/**
 *
 * @author Tran Tien Dung
 */

public class Ex15 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        long n = sc.nextLong();
        int x = sc.nextInt();
        System.out.println("SO VO MUA DUOC LA: " + n / x + " !!!!!");
    }
}
