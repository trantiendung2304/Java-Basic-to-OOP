import java.util.Scanner;

/**
 *
 * @author Tran Tien Dung
 */

public class Ex14 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        long a = sc.nextLong(), b = sc.nextLong();
        long totalNum = (b - a) + 1;
        System.out.println(totalNum);
    }
}
